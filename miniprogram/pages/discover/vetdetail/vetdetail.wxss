  height: 100px;
}
.vetPhoto{
  width: 100%;
  height: 100%;
}
.title-name{
  font-size: 24px;
  color:rgb(70,118,166);
  position:absolute;
  left: 160px;
}
.flex-item {
position:absolute;
right: 20px;
}
.base-info-line2{
  display: flex;
  position:absolute;
  left: 160px;
  margin-top:40px;
}
.certImage{
  float: right;
  width: 100px;
  height: 100px;
}
.certImg{
  width: 100%;
  height: 100%;
}
.weui-article__p{
  color:grey;
}
.caseTitle{
