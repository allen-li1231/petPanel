<!--page/userHome/pages/pets/pets.wxml-->
<view class="page" data-weui-theme="{{theme}}">
    <view class="page__bd">
      <mp-cells>
        <mp-slideview show="{{false}}" buttons="{{slideButtons}}"bindbuttontap="slideButtonTap">
          <navigator url="petedit/petedit" > 
                <mp-cell class="petProfile" >{{profileName}}档案</mp-cell>
                </navigator>
            </mp-slideview>
      </mp-cells>
        <navigator url="petadd/petadd" hover-class="ohter-navigator">
       <button class="addNew-btn">新建宠物信息</button>
      </navigator>
</view>
</view>

